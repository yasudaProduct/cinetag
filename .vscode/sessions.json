{
  "$schema": "https://cdn.statically.io/gh/nguyenngoclongdev/cdn/main/schema/v11/terminal-keeper.json",
  "theme": "tribe",
  "active": "default",
  "activateOnStartup": true,
  "keepExistingTerminals": false,
  "sessions": {
    "default": [
      [
        {
          "name": "frontend-terminal",
          "autoExecuteCommands": true,
          "icon": "terminal",
          "color": "terminal.ansiGreen",
          "commands": [
            "cd apps/frontend"
          ]
        },
        {
          "name": "backend-terminal",
          "autoExecuteCommands": true,
          "icon": "terminal",
          "color": "terminal.ansiBlue",
          "commands": [
            "cd apps/backend"
          ]
        }
      ],
      [
        {
          "name": "nextjs",
          "autoExecuteCommands": true,
          "icon": "browser",
          "color": "terminal.ansiGreen",
          "commands": [
            "cd apps/frontend",
            "npm run dev"
          ]
        },
        {
          "name": "gin",
          "autoExecuteCommands": true,
          "icon": "server",
          "color": "terminal.ansiBlue",
          "commands": [
            "cd apps/backend",
            "docker compose up -d postgres",
            "go run src/cmd/main.go"
          ]
        }
      ],
      {
        "name": "backend-test-terminal",
        "autoExecuteCommands": true,
        "icon": "terminal",
        "color": "terminal.ansiRed",
        "commands": [
          "cd apps/backend",
          "go test ./..."
        ]
      },
      {
        "name": "ngrok",
        "autoExecuteCommands": true,
        "icon": "browser",
        "color": "terminal.ansiYellow",
        "commands": [
          "ngrok http 8080"
        ]
      }
    ]
  }
}